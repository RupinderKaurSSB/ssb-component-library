{
  "name": "@statisticsnorway/ssb-component-library",
  "version": "1.0.2",
  "description": "Component library for SSB (Statistics Norway)",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && cross-env ENV=production webpack --mode production --config webpack.config.js --progress",
    "build:prod": "cross-env ENV=production rollup -c",
    "transpile": "rm -rf lib && cross-env ENV=production babel ./src --presets @babel/preset-env -d lib --ignore \"**/*.story.jsx, **/*.test.jsx\"",
    "build-css": "node-sass src/main.scss lib/styles.css",
    "start": "cross-env ENV=development start-storybook -p 6006",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:js": "eslint src/**/*.jsx",
    "lint:css": "stylelint src/**/*.jsx",
    "deploy": "npm publish"
  },
  "repository": {
